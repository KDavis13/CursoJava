package dummies;

public class DummyIdioma {

	// Atributos
	private String agenda = "Agenda";
	private String[] otros = { "Año", "Mes", "Semana", "Día" };
	private String generado = "Generado por";
	private String[] estado = {"Cerrado", "Disponible"};
	private String error = "Error";
	private String[] diasSemana = { "Lunes", "Martes", "Miercoles", "Jueves",
			"Viernes", "Sabado", "Domingo" };
	private char[] inicialesSemana = { 76, 77, 88, 74, 86, 83, 68 };
	private String[] meses = { "Enero", "Febrero", "Marzo", "Abril", "Mayo",
			"Junio", "Julio", "Junio", "Agosto", "Septiembre", "Octubre",
			"Noviembre", "Diciembre" };
	
	//Getters
	public String getAgenda() {
		return agenda;
	}
	
	public String[] getOtros() {
		return otros;
	}
	
	public String getGenerado() {
		return generado;
	}
	
	public String[] getEstado() {
		return estado;
	}
	
	public String getError() {
		return error;
